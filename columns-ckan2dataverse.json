{
  "dataset-name": {
      "pattern": "^name$"
  },
  "dataset title": {
      "pattern": "^title$"
  },
  "project title": {
      "pattern": "^ILRI_prjtitle$"
  },
  "alternativeURL": {
      "pattern": "^ILRI_prjwebsite$"
  },
  "Project Manager (First Author)": {
      "pattern": "^ILRI_prjpi|ILRI_actypi$",
      "case_sensitive": true,
      "deduplicate": true
  },
  "First and co-authors": {
      "pattern": "^(ILRI_prjstaff|ILRI_actystaff)$",
      "case_sensitive": true,
      "deduplicate": true
  },
  "Data Curator": {
      "pattern": "^ILRI_actycustodian$"
  },
  "Partners": {
      "pattern": "^(ILRI_prjpartners|ILRI_actypartners)$",
      "case_sensitive": true,
      "deduplicate": true
  },
  "Affiliation": {
      "pattern": "^ILRI_actyipownership$"
  },
  "Resources": {
      "pattern": ["^resources$", "^name$"],
      "case_sensitive": true,
      "deduplicate": true
  },
  "Format": {
      "pattern": ["^resources$", "^format$"],
      "case_sensitive": true,
      "deduplicate": true
  },
  "dsDescription (dsDescriptionValue)": {
      "pattern": "^(ILRI_prjabstract|notes)$",
      "deduplicate": true
  },
  "dsDescription (dsDescriptionValue beware commas)": {
      "pattern": "^ILRI_actyotheruse$",
      "deduplicate": true
  },
  "geographicCoverage (otherGeographicCoverage)": {
      "pattern": "^(ILRI_prjregions|ILRI_actyregions|ILRI_actyprojection)$",
      "deduplicate": true
  },
  "GPS coordinates": {
      "pattern": "^ILRI_actyboundboxcenter$"
  },
  "geographicCoverage (country)": {
      "pattern": "^(ILRI_prjcountries|ILRI_actycountries)$",
      "deduplicate": true
  },
  "keyword": {
      "pattern": "^(ILRI_prjsubjects|ILRI_prjspecies|ILRI_actyspecies)$"
  },
  "keyword (AGROVOC)": {
      "pattern": ["^tags$", "name"],
      "deduplicate": true
  },
  "Donor": {
      "pattern": "^ILRI_prjdonor$"
  },
  "Donor agreement number": {
      "pattern": "^ILRI_prjgrant$"
  },
  "license": {
      "pattern": "^license_id$"
  },
  "producer (producerAbbreviation)": {
      "pattern": ["^organization$", "^name$"]
  },
  "crp": {
      "pattern": ["^groups$", "display_name"],
      "case_sensitive": true,
      "deduplicate": true
  },
  "geographicBoundingBox": {
      "pattern": "^ILRI_actyboundbox$"
  },
  "notesText": {
      "pattern": "^ILRI_actycitationacknowledge$"
  },
  "datasetContact (datasetContactName)": {
      "pattern": "^(ILRI_actycustodian|ILRI_actycontactperson)$",
      "deduplicate": true
  },
  "datasetContact (datasetContactEmail)": {
      "pattern": "^(ILRI_actycustodianemail|ILRI_actycontactemail|ILRI_prjpiemail|ILRI_actypiemail)$",
      "deduplicate": true
  },
  "date of online publication": {
      "pattern": "^ILRI_actydatavailable$"
  },
  "date of online publication 2 (if no ILRI_actydatavailable)": {
      "pattern": "^metadata_created$"
  },
  "Project start date": {
      "pattern": "^ILRI_prjsdate$"
  },
  "Project end date": {
      "pattern": "^ILRI_prjedate$"
  },
  "Research period start": {
      "pattern": "^ILRI_actydatecollected$"
  },
  "Research period end": {
      "pattern": "^ILRI_actydatecollectedend$"
  },
  "socialScienceNotes (socialScienceNotesType=Consent, socialScienceNotesText=Yes)": {
      "pattern": "^ILRI_actyfarmconsent$"
  },
  "ILRI_actymapextent (ignore)": {
      "pattern": "^ILRI_actymapextent$"
  },
  "Geographical Administrative Unit": {
      "pattern": "^ILRI_actynatlevel$"
  }
}
